{
    "manifest_version" : 2,
    "version": "0.1",
    "name": "khamsatBot",
    "description" : "extension helper for khamsat bot",
    "permissions": [
        "cookies",
        "tabs",
        "http://khamsat.com/*", 
        "https://khamsat.com/*" 
    ],
    "browser_action" : {
        "default_icon":"16.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}